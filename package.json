{
  "name": "pmjakarta-server",
  "version": "1.0.0",
  "main": "server/server.js",
  "scripts": {
    "lint": "eslint .",
    "start": "node .",
    "posttest": "npm run lint && nsp check",
    "build:sdk": "./node_modules/.bin/lb-sdk server/server.js ./client/sdk -l angular2 -d ng2web --io enabled",
    "server:deploy": "slc deploy --service pmjakarta-server http://192.168.0.100",
    "server:deploy-testing": "slc deploy --service pmjakarta-testing http://192.168.0.100",
    "server:deploy-development": "slc deploy --service pmjakarta-development http://192.168.0.100",
    "server:build": "slc build --npm",
    "server:status": "slc ctl --control http://192.168.0.100 status",
    "server:log-mail": "slc ctl --control http://192.168.0.100 log-dump  pmjakarta-mail"
  },
  "dependencies": {
    "@mean-expert/boot-script": "^1.0.0",
    "@mean-expert/fireloop": "^1.0.0-beta.2.4",
    "@mean-expert/loopback-component-realtime": "1.0.0-rc.9.5",
    "@mean-expert/loopback-sdk-builder": "2.1.0-rc.13.5",
    "@mean-expert/loopback-stats-mixin": "1.2.2",
    "@types/node": "8.0.24",
    "async": "^2.3.0",
    "body-parser": "^1.17.1",
    "compression": "1.7.0",
    "cors": "2.8.4",
    "helmet": "3.8.1",
    "https": "1.0.0",
    "jimp": "^0.2.27",
    "loopback": "3.10.1",
    "loopback-boot": "2.26.1",
    "loopback-component-explorer": "^4.2.0",
    "loopback-component-storage": "^3.2.0",
    "loopback-connector-mongodb": "3.2.1",
    "loopback-connector-mysql": "5.1.0",
    "loopback-ds-timestamp-mixin": "^3.4.0",
    "moment": "^2.18.1",
    "pug": "^2.0.0-rc.2",
    "serve-favicon": "^2.4.2",
    "socket.io": "2.0.3",
    "socket.io-adapter-mongo": "1.0.1",
    "socket.io-redis": "5.1.0",
    "strong-error-handler": "2.2.0",
    "ts-node": "3.3.0",
    "typescript": "2.4.2",
    "xmlhttprequest": "^1.8.0"
  },
  "devDependencies": {
    "eslint": "4.5.0",
    "eslint-config-loopback": "^8.0.0",
    "jshint": "^2.9.4",
    "nsp": "2.7.0"
  },
  "repository": {
    "type": "git",
    "url": "http://imron.rosdiana@git.dataon.com/pmjakarta/pmjakarta-server.git"
  },
  "license": "UNLICENSED",
  "description": "pmjakarta-server"
}
