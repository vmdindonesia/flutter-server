    "async": "^2.3.0",
    "body-parser": "^1.17.1",
    "compression": "^1.0.3",
    "cors": "^2.8.3",
    "helmet": "^3.5.0",
    "jimp": "^0.2.27",
    "loopback": "3.6.0",
    "loopback-boot": "^2.24.0",
    "loopback-component-explorer": "^4.2.0",
    "loopback-component-storage": "^3.2.0",
    "loopback-connector-mysql": "^4.0.0",
    "loopback-ds-timestamp-mixin": "^3.4.0",
